<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:boyaaKefu="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.boyaa.customer.service.widget.BoyaaValiditedItemLayout
        android:id="@+id/bvitem_common_ip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        boyaaKefu:editTextHint="@string/boyaa_kefu_common_login_ip"
        boyaaKefu:enabled="true"
        boyaaKefu:errorText="@string/boyaa_kefu_input_error"
        boyaaKefu:indicatorBackgroud="@drawable/boyaa_kefu_input_indicator"
        boyaaKefu:isShowPic="true" />

    <com.boyaa.customer.service.widget.BoyaaValiditedItemLayout
        android:id="@+id/bvitem_last_amount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        boyaaKefu:editTextHint="@string/boyaa_kefu_last_amount"
        boyaaKefu:enabled="true"
        boyaaKefu:errorText="@string/boyaa_kefu_input_error"
        boyaaKefu:indicatorBackgroud="@drawable/boyaa_kefu_input_indicator"
        boyaaKefu:inputType="boyaa_kefu_number"
        boyaaKefu:isShowPic="true" />

    <com.boyaa.customer.service.widget.BoyaaValiditedItemLayout
        android:id="@+id/bvitem_first_login_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        boyaaKefu:editTextHint="@string/boyaa_kefu_first_login_date"
        boyaaKefu:editTextId="@+id/et_first_login_date"
        boyaaKefu:enabled="true"
        boyaaKefu:errorText="@string/boyaa_kefu_input_error_date"
        boyaaKefu:indicatorBackgroud="@drawable/appkefu_disclosure_arrow_normal"
        boyaaKefu:inputType="boyaa_kefu_none"
        boyaaKefu:isShowPic="true" />

    <com.boyaa.customer.service.widget.BoyaaValiditedItemLayout
        android:id="@+id/bvitem_last_login_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        boyaaKefu:editTextHint="@string/boyaa_kefu_last_login_date"
        boyaaKefu:editTextId="@+id/et_last_login_date"
        boyaaKefu:enabled="true"
        boyaaKefu:errorText="@string/boyaa_kefu_input_error_date"
        boyaaKefu:indicatorBackgroud="@drawable/appkefu_disclosure_arrow_normal"
        boyaaKefu:inputType="boyaa_kefu_none"
        boyaaKefu:isShowPic="true" />

    <com.boyaa.customer.service.widget.BoyaaValiditedItemLayout
        android:id="@+id/bvitem_safe_box_amount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        boyaaKefu:editTextHint="@string/boyaa_kefu_safe_box_amount"
        boyaaKefu:enabled="true"
        boyaaKefu:errorText="@string/boyaa_kefu_input_error"
        boyaaKefu:indicatorBackgroud="@drawable/boyaa_kefu_input_indicator"
        boyaaKefu:inputType="boyaa_kefu_number"
        boyaaKefu:isShowPic="false" />

