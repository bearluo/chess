<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <include layout="@layout/boyaa_kefu_dialog_picker_header"/>
    
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginBottom="1dp"
        android:background="#999999" />
    
    <LinearLayout
        android:layout_width="320dip"
        android:layout_height="195dip"
        android:layout_marginLeft="20dip"
        android:orientation="horizontal" >

        <com.boyaa.customer.service.widget.WheelView
	        android:id="@+id/boyaa_kefu_dialog_inform_type"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:layout_marginTop="30dip"
	        android:layout_marginBottom="43dip"
	        app:boyaa_kefu_itemNumber="5"
	        app:boyaa_kefu_lineColor="#666666"
	        app:boyaa_kefu_maskHight="50dp"
	        app:boyaa_kefu_noEmpty="true"
	        app:boyaa_kefu_normalTextColor="#666666"
	        app:boyaa_kefu_normalTextSize="15sp"
	        app:boyaa_kefu_selectedTextColor="#000000"
	        app:boyaa_kefu_selectedTextSize="20sp"
	        app:boyaa_kefu_unitHight="30dp"
	        />

    </LinearLayout>    

    

</LinearLayout>